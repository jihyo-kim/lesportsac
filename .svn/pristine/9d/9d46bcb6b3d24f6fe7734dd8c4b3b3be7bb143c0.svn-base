#sitemap {background:url("../../lib/images/common/bg_site.gif")repeat-x scroll 50% 0; border-top:2px #D90094 solid; border-bottom:1px #CCC solid; width:100%;	height:356px; }
#sitemap .site_inner{max-width:960px; margin:0 auto; padding:35px 0 0}
#sitemap .site_inner>ul>li{float:left; width:160px;  }
#sitemap .site_inner>ul>li>h1{font-family:"Arial"; font-size:0.85em; color: #D900A0; margin-bottom:15px}
#sitemap .site_inner>ul>li>ul>li{margin-bottom:3px}
#sitemap .site_inner>ul>li>ul>li>a{font-family:"Nanum Gothic"; font-size:0.75em; color:#666; font-weight:bold}

#footer{height:130px}
#footer .footer_inner{position:relative; top 0px; left:0px; width:960px; margin:0 auto; padding:25px 0px 0px; } 
#footer .footer_inner .footer_left {float:left}
#footer .footer_inner .footer_left .address{font-family:"Nanum Gothic"; font-size:0.75em; line-height:20px; }
#footer .footer_inner .footer_left .copyright{margin-top:25px; font-family:"Nanum Gothic"; font-size:0.75em; color:#757575 }

#footer .footer_inner .footer_right .sns_box{float:right}
#footer .footer_inner .footer_right .sns_box>a{display:block; float:left; margin-left: 20px; width:33px; height:33px; text-indent:-9999px;}
#footer .footer_inner .footer_right .sns_box .icon_facebook{background:url("../../lib/images/common/ico_facebook.png")no-repeat}
#footer .footer_inner .footer_right .sns_box .icon_facebook:hover{background:url("../../lib/images/common/ico_facebook_on.png")no-repeat}
#footer .footer_inner .footer_right .sns_box .icon_blog{background:url("../../lib/images/common/ico_BLOG.png")no-repeat}
#footer .footer_inner .footer_right .sns_box .icon_blog:hover{background:url("../../lib/images/common/ico_BLOG_on.png")}
#footer .footer_inner .footer_right .sns_box .icon_instagram{background:url("../../lib/images/common/ico_instagram2.png")no-repeat}
#footer .footer_inner .footer_right .sns_box .icon_instagram:hover{background:url("../../lib/images/common/ico_instagram2_on.png")}
#footer .footer_inner .footer_right .familysite{position:absolute; right:0px; top:75px; }
#footer .footer_inner .footer_right .familysite>a{display:block; padding-left:10px; background:url("../../lib/images/common/bg_arr_select.png")no-repeat right ; border:1px #757575 solid; font-family:"FuturaBT-Medium","Nanum Gothic"; font-size:0.75em; color:#757575; line-height:20px; width:130px; height:20px; }










