#container {height:100%;	width:960px;	margin:0 auto;}

#container .main_slide{position:relative; margin-top:40px; margin-bottom:40px}
#container .main_slide>.slide_img{position:relative; width:960px; height:500px; overflow:hidden; }
#container .main_slide>.slide_img>li{float:left; display:inline-block; width:960px; height:500px;   }
#container .main_slide>.slide_img>li>div{display:none; position:absolute;}
#container .main_slide>.slide_img>li:first-child div{ display:block }
#container .main_slide>.slide_img>li img{width:100%; }
#container .main_slide>.slide_img>li:hover{opacity:0.7; }

#container .main_slide .paging{ margin-top:20px; text-align:center;  }
#container .main_slide .paging>li{display: inline-block; }
#container .main_slide .slide_button{display: block; text-indent:-9999px; background:url("../../lib/images/main/btn_paging.png") no-repeat center; width:11px; height:11px;}

#container button{display:block; position:absolute; top:47%; z-index:1; margin-top:-25px;text-indent:-99999px; width:50px; height:50px; cursor:pointer}
#container button.next{position:absolute; right:20px; background:url("../../lib/images/main/btn_next.png")}
#container button.prev{position:absolute; left:20px; background:url("../../lib/images/main/btn_prev.png")}

#container .title_bar{margin-bottom:20px; background:url("../../lib/images/main/bg_title.gif")repeat-x scroll 0 55%; text-align:center}
#container .title_bar .main_title{background:#FFF; padding:0px 30px; font-family:"FuturaHvBTHeavy"; font-weight:normal; font-size:0.75em; color:#333}

#container .issue{position:relative; width:960px; height:500px; }
#container .issue>ul>li{float:left;  width:320px; height:420px;}
#container .issue>ul>li>a>img{width:100%}
#container .issue>ul>li>a:hover{opacity:0.7}
#container .issue>ul>li>a>span{display:block; padding:20px 20px 0; height:70px;font-size:0.80em; line-height:18px; letter-spacing:-0.5px }
#container .issue>ul>li>a>span>strong{margin-bottom:3px; line-height:14px; font-family:"Nanum Gothic"; font-size:1.2em }

#container .section .main_list{margin-top:15px; margin-bottom:70px; overflow:hidden}
#container .section .main_object{display:block; position:relative; float:left; width:25%}
#container .section .layer{position:absolute;top:0; left:0; z-index: 10; width:45px; height:45px}
#container .section .layer>img{width:100%; margin-bottom:5px; }
#container .section .object>a>img{width:100%}
#container .section .object>a>img:hover{opacity:0.7}

#container .collection>ul{overflow:hidden; }
#container .collection .collection_object{float:left;  margin:0 12px 15px 0; overflow:hidden; width:231px; height:230px}
#container .collection .last{margin-right:0px}
#container .collection>ul>li>div{position:relative; border-radius:50%; width:100%; height:100%; overflow:hidden}
#container .collection>ul>li>div>a>img{width:100%}
#container .collection>ul>li>div>a>span{position:absolute; top:40%; left:0; padding:10px 0px; background:url("../../lib/images/collection/bg_collection.png"); z-index: 999; width:100%; text-align:center; color:#fff; line-height:21px; font-family:"FuturaBT-Medium"; font-size:1.1em }

#container dl{text-align: center; height:110px}
#container dt{margin-bottom:5px;font-size:0.85em; font-weight:bold}
#container dd{color:#666; font-size:0.85em}
#container dd.price1{color:#A2A2A2; font-size:0.85em; text-decoration:line-through}
#container dd.price2{color:#333; font-size:0.85em}

#top{position:fixed; bottom: 50px; right:95px; background: url("../../lib/images/common/btn_top.png")no-repeat; width: 45px; height: 45px}
#top span{display: block;   text-indent: -9999px}
